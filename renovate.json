{
  "extends": [
    "@nuxtjs"
  ],
  "baseBranches": [
    "main"
  ],
  "automerge": true,
  "rangeStrategy": "pin",
  "major": {
    "automerge": false
  },
  "ignoreDeps": [
    "postcss-preset-env"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "snyk"
      ],
      "groupName": "snyk",
      "automerge": true,
      "automergeType": "branch"
    }
  ]
}
