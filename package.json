    "reqlim": "0.0.0",
    "rxjs": "7.5.2",
    "snyk": "1.840.0"
  },
  "optionalDependencies": {
    "picturefill": "3.0.3",
    "custom-event-polyfill": "1.0.7",
    "default-passive-events": "2.0.0",
    "domtokenlist-shim": "1.2.0",
    "intersection-observer": "0.12.0",
    "requestidlecallback": "0.3.0"
  },
  "snyk": true
}
