    "@nuxtjs/storybook": "3.2.0",
    "@packtracker/webpack-plugin": "2.3.0",
    "@storybook/addon-a11y": "6.1.8",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "babel-preset-vue": "2.0.2",
    "branch-name-lint": "1.4.0",
    "core-js": "3.8.0",
    "eslint": "7.14.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-security": "1.4.0",
    "express": "4.17.1",
    "husky": "4.3.0",
    "license-webpack-plugin": "2.3.1",
    "lint-staged": "10.5.2",
    "lost": "8.3.1",
    "nodeify": "1.0.1",
    "postcss-custom-media": "7.0.8",
    "postcss-momentum-scrolling": "2.8.20",
    "postcss-nested": "5.0.1",
    "postcss-normalize": "9.0.0",
    "postcss-object-fit-images": "1.1.2",
    "postcss-url": "9.0.0",
    "reqlim": "0.0.0",
    "rucksack-css": "1.0.2",
    "stylelint": "13.8.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-lost": "0.0.3",
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-config-standard": "20.0.0",
    "svgo-loader": "2.2.1",
    "upath": "2.0.1",
    "vue-svg-loader": "0.16.0",
    "webpack-virtual-modules": "0.4.1"
  },
  "dependencies": {
    "@js-basics/vector": "1.56.8",
    "@nuxtjs/axios": "5.12.2",
    "@nuxtjs/pwa": "3.2.2",
    "@nuxtjs/robots": "2.4.2",
    "nuxt": "2.14.7",
    "nuxt-font-loader-strategy": "1.1.1",
    "nuxt-i18n": "6.15.4",
