    "commitlint": "19.2.1",
    "eslint": "9.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-no-secrets": "0.8.9",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-security": "3.0.0",
    "eslint-plugin-xss": "0.1.12",
    "husky": "9.0.11",
