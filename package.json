{
  "name": "gp-vue-boilerplate",
  "version": "1.18.2",
  "description": "Grabarz & Partner Boilerplate",
  "author": "Stephan Gerbeth",
  "contributors": [
    {
      "name": "Boris Horn",
      "email": "boris.horn@grabarzundpartner.de"
    }
  ],
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "dev": "cross-env NODE_ENV=development node ./env/server.js",
    "build": "npx nuxt build --config-file env/nuxt.config.js",
    "static": "npm run build && cross-env NODE_ENV=static nuxt generate --config-file env/nuxt.config.js",
    "start": "cross-env NODE_ENV=production node ./env/server.js",
    "production": "npm run static && npm run start",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es": "eslint --fix --ext .js,.vue --ext .json --ignore-path .gitignore ./",
    "lint:css": "npx stylelint \"src/**/*.vue\"",
    "audit": "npm audit --json | npx npm-audit-html --output reports/audit.html; exit ${PIPESTATUS[0]}",
    "browserslist": "npx browserslist",
    "cypress:open": "npx cypress open",
    "cypress:run": "npx cypress run",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once",
    "storybook:dev": "start-storybook -p 6006 -c .storybook",
    "storybook:prod": "build-storybook -c .storybook -o dist/storybook",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^7.6.0",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "@nuxtjs/sitemap": "^1.2.0",
    "@storybook/addon-a11y": "^5.0.11",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-viewport": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/vue": "^5.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "branch-name-lint": "^1.0.5",
    "cross-env": "^5.2.0",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.17.1",
    "httpolyglot": "^0.1.2",
    "husky": "^2.3.0",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webp": "^5.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "license-webpack-plugin": "^2.1.1",
    "multi-loader": "^0.1.0",
    "node-gyp": "^4.0.0",
    "node-notifier": "^5.4.0",
    "open": "^6.3.0",
    "postcss-custom-media": "^7.0.8",
    "postcss-loader": "^3.0.0",
    "postcss-momentum-scrolling": "^1.6.12",
    "postcss-nested": "^4.1.2",
    "postcss-normalize": "^7.0.1",
    "postcss-object-fit-images": "^1.1.2",
    "rucksack-css": "^1.0.2",
    "stylelint": "^10.0.1",
    "stylelint-config-css-modules": "^1.4.0",
    "stylelint-config-recess-order": "^2.0.2",
    "stylelint-config-standard": "^18.3.0",
    "svg-sprite-loader": "^4.1.6",
    "svgo-loader": "^2.2.0",
    "terser": "^4.0.0",
    "webpack-virtual-modules": "0.1.10"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "@nuxtjs/robots": "^2.1.0",
    "default-passive-events": "^1.0.10",
    "mime": "^2.4.3",
    "nuxt": "^2.7.1",
    "nuxt-i18n": "^5.12.3",
    "object-fit-images": "github:StephanGerbeth/object-fit-images",
    "rxjs": "^6.5.2",
    "svg4everybody": "^2.1.9",
    "snyk": "^1.161.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/gp-vue-boilerplate.git"
  },
  "snyk": true
}
