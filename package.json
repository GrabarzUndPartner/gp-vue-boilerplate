{
  "name": "gp-vue-boilerplate",
  "version": "1.13.0",
  "description": "Grabarz & Partner Boilerplate",
  "author": "Stephan Gerbeth",
  "contributors": [
    {
      "name": "Boris Horn",
      "email": "boris.horn@grabarzundpartner.de"
    }
  ],
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "dev": "cross-env NODE_ENV=development node ./env/server.js",
    "build": "cross-env NODE_ENV=build nuxt build --config-file env/nuxt.config.js",
    "static": "npm run build && cross-env NODE_ENV=static nuxt generate --config-file env/nuxt.config.js",
    "start": "cross-env NODE_ENV=production node ./env/server.js",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:es": "eslint --fix --ext .js,.vue --ext .json --ignore-path .gitignore ./",
    "lint:css": "npx stylelint 'src/**/*.vue'",
    "audit": "npm audit --json | npx npm-audit-html --output reports/audit.html; exit ${PIPESTATUS[0]}",
    "browserslist": "npx browserslist",
    "wdio": "npx wdio ./test/wdio/wdio.conf.js",
    "wdio-local": "npx wdio ./test/wdio/wdio.conf.local.js",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@fullhuman/postcss-purgecss": "^1.1.0",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@mapbox/stylelint-processor-arbitrary-tags": "^0.2.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-vue-app": "^2.0.0",
    "branch-name-lint": "^1.0.5",
    "chai": "^4.2.0",
    "chromedriver": "^2.43.1",
    "cross-env": "^5.2.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.8.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-vue": "^4.7.1",
    "express": "^4.16.4",
    "geckodriver": "^1.14.1",
    "httpolyglot": "^0.1.2",
    "husky": "^1.1.3",
    "imagemin-jpeg-recompress": "^5.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webp": "^4.1.0",
    "imagemin-webpack-plugin": "^2.3.0",
    "mocha": "^5.2.0",
    "multi-loader": "^0.1.0",
    "node-gyp": "^3.8.0",
    "node-notifier": "^5.3.0",
    "opn": "^5.4.0",
    "postcss-custom-media": "^7.0.7",
    "postcss-loader": "^3.0.0",
    "postcss-momentum-scrolling": "^1.4.8",
    "postcss-nested": "^4.1.0",
    "postcss-normalize": "^7.0.1",
    "postcss-object-fit-images": "^1.1.2",
    "postcss-pseudo-content-insert": "^0.3.0",
    "rucksack-css": "^1.0.2",
    "selenium-standalone": "^6.15.3",
    "semantic-release": "^15.10.7",
    "semantic-release-cli": "^4.0.9",
    "stylelint": "^9.7.1",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-fill-loader": "0.0.8",
    "svg-sprite-loader": "^4.1.3",
    "svgo-loader": "^2.2.0",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-sauce-service": "^0.4.12",
    "wdio-screenshot": "^0.6.0",
    "wdio-selenium-standalone-service": "0.0.10",
    "wdio-spec-reporter": "^0.1.5",
    "webdriverio": "^4.14.0",
    "webp-loader": "^0.4.0",
    "webpack-virtual-modules": "0.1.10"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.4",
    "@nuxtjs/pwa": "^2.6.0",
    "default-passive-events": "^1.0.10",
    "mime": "^2.3.1",
    "nuxt": "^2.2.0",
    "nuxt-i18n": "^5.4.4",
    "object-fit-images": "github:StephanGerbeth/object-fit-images",
    "rxjs": "^6.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrabarzUndPartner/gp-vue-boilerplate.git"
  }
}
