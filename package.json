    "@snyk/protect": "1.1292.4",
    "commitlint": "19.4.0",
    "eslint": "9.9.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-no-secrets": "1.0.2",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-security": "3.0.1",
